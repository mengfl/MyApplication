<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/color_0a2343"
     android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/fra_inout_ll"
        android:padding="@dimen/space_10"
        android:layout_marginTop="@dimen/space_25"
        android:layout_marginRight="@dimen/space_25"
        android:layout_marginLeft="@dimen/space_25"
        android:background="@drawable/shape_button_white"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/fra_inout_tv_title"
            style="@style/txt_a2faff_16"
            android:text="1、综合极端-登入登入 (第一轮)"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/fra_inout_tv_content"
            android:lineSpacingExtra="2dp"
            android:layout_marginTop="@dimen/space_5"
            style="@style/txt_a2faff_14"
            android:text="请将终端上电启动，进行第?次登入登出测试。友情提示：检测过程总共需要进行3轮登入登出测试。"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/fra_inout_tv_time"
            android:layout_marginTop="@dimen/space_5"
            style="@style/txt_fe_12"
            android:gravity="right"
            android:text="01:00"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <Button
        android:paddingTop="@dimen/space_2"
        android:paddingBottom="@dimen/space_2"
        android:layout_below="@id/fra_inout_ll"
        android:background="@drawable/shape_button_main"
        android:textColor="@color/color_black"
         android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/space_25"
        android:text="@string/zbjx"
        android:id="@+id/fra_inout_btn_start"
        android:layout_width="200dp"
        android:layout_height="44dp" />

    <TextView
        android:paddingBottom="@dimen/space_10"
        android:paddingTop="@dimen/space_10"
        android:gravity="center"
        style="@style/txt_a2faff_14"
        android:id="@+id/fra_inout_tv_jump"
        android:layout_alignParentBottom="true"
        android:text="@string/fhxzcsxm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <View
       android:background="@color/color_939699"
        android:layout_above="@id/fra_inout_tv_jump"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_1px"  />


</RelativeLayout>