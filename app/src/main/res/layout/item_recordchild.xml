<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_marginTop="@dimen/space_5"
    android:paddingLeft="@dimen/space_10"
    android:background="@color/color_0a2343"
    android:paddingRight="@dimen/space_10"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/recordchild_tv_name"
        android:text="登入登出第一轮"
        style="@style/txt_a2faff_14"
        android:paddingBottom="@dimen/space_5"
        android:paddingTop="@dimen/space_5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            style="@style/txt_fe_12"
            android:text="检测状态："
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/recordchild_tv_status"
            android:layout_weight="1"
            android:text="已完成"
            style="@style/txt_a2faff_12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/recordchild_tv_begintime"
            style="@style/txt_fe_12"
            android:text="开始时长：2017-04-06 11:11:11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginBottom="@dimen/space_5"
        android:layout_marginTop="@dimen/space_5"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            style="@style/txt_fe_12"
            android:text="检测结果："
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/recordchild_tv_result"
            android:layout_weight="1"
            android:text="成功"
            android:textSize="@dimen/font_size_12"
            android:textColor="@color/color_a2f1ff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/recordchild_tv_overtime"
            style="@style/txt_fe_12"
            android:text="结束时长：2017-04-06 11:11:11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <View
        android:background="#2c4059"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_1px" />
</LinearLayout>